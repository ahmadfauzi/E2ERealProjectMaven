<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

  <listeners>
  	<listener class-name="Academy.listeners"></listener>
  	<listener class-name="resources.ExtentReporterNG"></listener>
  </listeners>
  
  <test thread-count="5" name="Validating User Login">
    <classes>
      
      <class name="Academy.HomePage"/>
    </classes>
  </test>
  
  <test thread-count="5" name="Validating Navigation bar">
    <classes>	
      <class name="Academy.ValidateNavigationBar"/>
    </classes>
  </test>
  
   <test thread-count="5" name="Validating Title">
    <classes>
      <class name="Academy.ValidateTitle"/>
    </classes>
  </test>
</suite> <!-- Suite -->
